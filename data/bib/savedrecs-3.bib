@article{ WOS:001528960800001,
Author = {Li, Ling and Meng, Zhongxiu and Zhao, Wenhao and Du, Xinyi},
Title = {Mapping the digital innovation ecosystem with machine learning:
   efficiency, drivers, and regional heterogeneity across China's provinces},
Journal = {INFORMATION PROCESSING \& MANAGEMENT},
Year = {2025},
Volume = {62},
Number = {6},
Month = {NOV},
Abstract = {The digital innovation ecosystem is a cornerstone of contemporary
   economic, technological, and social progress. A precise understanding of
   its efficiency and driving factors is crucial for enhancing the
   innovation capabilities of regional digital economies. This study
   evaluates the Digital Innovation Ecosystem Efficiency (DIEE) across 30
   provinces in China from 2018 to 2022, employing the Super-SBM model and
   the Malmquist-Luenberger model. Using Python-based machine learning
   techniques, the provinces are categorized into four heterogeneous
   groups, and the Classification and Regression Tree (CART) decision tree
   classification method identifies the primary driving factors of digital
   innovation development at both the overall and subgroup levels. The
   findings indicate that six factors, including the number of scientific
   and technical personnel and government performance, significantly
   influence DIEE across all provinces. Regionally, the number of internet
   broadband access ports and the total volume of telecommunications
   services exert the most significant driving effects on the All-round
   Developed Provinces group. Market institutions, internet broadband
   access ports, and urbanization levels significantly impact the Policy
   Developed Provinces group. In contrast, the total volume of
   telecommunications services and urbanization levels are critical for the
   Culture-Driven Provinces group and the Weak Provinces group,
   respectively. Based on these results, this study provides targeted
   strategies to enhance digital innovation capacity, offering theoretical
   insights and practical guidance for regions aiming to develop tailored
   digital economy initiatives.},
