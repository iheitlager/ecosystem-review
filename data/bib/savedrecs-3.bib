@article{ WOS:001537892300001,
Author = {Fu, Lihua and Wang, Xuan and Long, Dan},
Title = {Do board networks drive innovation performance? The moderating role of
   digital transformation},
Journal = {TECHNOLOGY ANALYSIS \& STRATEGIC MANAGEMENT},
Year = {2025},
Month = {2025 JUL 29},
Abstract = {Digital economy brings great changes to the effect of board networks.
   However, the relationship between board networks and innovation has not
   yet been sufficiently understood in the context of digital
   transformation. Therefore, from the perspectives of social network
   theory and digital empowerment, this study explores the effect of board
   networks on innovation performance and the moderating role of digital
   transformation. Using a dataset of listed companies from 2007 to 2020,
   we find that board network centrality and structural hole can
   significantly improve enterprise innovation performance, and enterprise
   digital transformation enhances the relationship between board networks
   and innovation performance. This study provides insights into the
   innovation effects of board networks in the context of digital
   transformation.},
