@article{ WOS:001447655000001,
Author = {Meng, Yongheng and Zhang, Yue and Hou, Zhiping and Zhang, Xianyan and
   Liu, Fangkun},
Title = {Niche fitness of digital innovation ecosystems: evaluation, dynamic
   evolution and spatial effects},
Journal = {EUROPEAN JOURNAL OF INNOVATION MANAGEMENT},
Year = {2025},
Month = {2025 MAR 20},
Abstract = {PurposeThis study aims to explore the current development status,
   spatiotemporal evolution, and spatial effects of niche fitness in
   China's regional digital innovation ecosystems (DIEs), providing
   guidance for coordinated regional digital
   innovation.Design/methodology/approachUtilizing ecological niche theory,
   this study develops an evaluation system for DIEs niche fitness,
   calculates the niche fitness and evolutionary momentum across 31
   provinces (including municipalities and autonomous regions, excluding
   Hong Kong, Macau and Taiwan) of China from 2014 to 2022 and analyzes
   their spatiotemporal characteristics. And a dynamic spatial Durbin model
   is employed to assess spatial effects.FindingsThe findings reveal that
   from 2014 to 2022, the niche fitness of China's regional DIEs exhibited
   an overall upward trend, particularly in the eastern regions. Fitness
   was higher in the east and south and lower in the west and north.
   Resource and technology fitness steadily increased, while innovation
   community fitness slightly declined. Both overall and individual
   ecological element niche fitness demonstrate significant temporal
   inertia, with the overall niche fitness also exhibiting spatial
   spillover effects.Originality/valueThis study applies ecological niche
   theory to DIEs, offering a novel perspective on their dynamics and
   providing theoretical support for optimizing innovation policies,
   promoting regional balanced development and advancing the twin
   transition process.},
