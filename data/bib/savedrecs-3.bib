@article{ WOS:000588843700001,
Author = {Nasiri, Mina and Saunila, Minna and Ukko, Juhani and Rantala, Tero and
   Rantanen, Hannu},
Title = {Shaping Digital Innovation Via Digital-related Capabilities},
Journal = {INFORMATION SYSTEMS FRONTIERS},
Year = {2023},
