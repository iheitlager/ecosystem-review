@article{ WOS:000732684100001,
Author = {Rocha, Clarissa and Quandt, Carlos and Deschamps, Fernando and Philbin,
   Simon and Cruzara, Giovani},
Title = {Collaborations for Digital Transformation: Case Studies of Industry 4.0
   in Brazil},
